{
  "rewrites": [
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/landing",
      "destination": "/"
    },
    {
      "source": "/subscribe/:path*",
      "destination": "/"
    },
    {
      "source": "/unsubscribe/:path*",
      "destination": "/"
    },
    {
      "source": "/gameplay/:path*",
      "destination": "/"
    },
    {
      "source": "/cards/:path*",
      "destination": "/"
    },
    {
      "source": "/kinbrold/:path*",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}